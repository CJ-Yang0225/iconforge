{
  "name": "@iconforge/vue",
  "version": "0.3.0",
  "private": true,
  "description": "Vue runtime utilities for IconForge",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CJ-Yang0225/iconforge.git",
    "directory": "packages/vue"
  },
  "keywords": [
    "vue",
    "icon",
    "svg"
  ],
  "author": "CJ Yang",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "vue": "^3.3.0",
    "vue-tsc": "^3.1.8"
  }
}
