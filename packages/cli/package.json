{
  "name": "@iconforge/cli",
  "version": "0.2.0",
  "description": "CLI tool for IconForge - Generate type-safe React icon components from SVG files",
  "bin": {
    "iconforge": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CJ-Yang0225/iconforge.git",
    "directory": "packages/cli"
  },
  "keywords": [
    "svg",
    "icon",
    "cli",
    "react",
    "typescript",
    "code-generator"
  ],
  "author": "CJ Yang",
  "license": "MIT",
  "homepage": "https://github.com/CJ-Yang0225/iconforge#readme",
  "bugs": {
    "url": "https://github.com/CJ-Yang0225/iconforge/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs --dts",
    "dev": "tsup src/index.ts --format cjs --dts --watch",
    "test": "vitest"
  },
  "dependencies": {
    "@iconforge/core": "workspace:*",
    "@types/fs-extra": "^11.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.0",
    "jiti": "^2.6.1",
    "ora": "^8.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.0"
  }
}
